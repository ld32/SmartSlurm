function unExport() {
    path=`which $1 2>/dev/null`; path=${path%/*}
    [ -z "$path" ] && echo "Usage: unExport <command from that path>" && exit
    [[ $PATH == *$path:* ]] && eval "export PATH="${PATH//$path:/}""
    [[ $PATH == *:$path* ]] && eval "export PATH="${PATH//:$path/}""
    path="$path/"
    [[ $PATH == *$path:* ]] && eval "export PATH="${PATH//$path:/}""
    [[ $PATH == *:$path* ]] && eval "export PATH="${PATH//:$path/}""
}
export unExport