configfile: "config.yaml"

SAMPLES = ["A", "B"]

rule all:
    input:
        "report.html"

rule find_1234:
    input:
        "numbers1.txt"
    output:
        "1234.1.txt"
    shell:
        "findNUmber.sh 1234 {input} > {output}"

rule find_5678:
    input:
        "numbers1.txt"
    output:
        "5678.1.txt"
    threads: 1
    shell:
        "findNUmber.sh 5678 {input} > {output}"

rule merge:
    input:
        "1234.1.txt", 
        '5678.1.txt'
    output:
        "all.txt"
    shell:
        "cat {input} > {output}"
